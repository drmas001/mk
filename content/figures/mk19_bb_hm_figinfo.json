"number":10,
"bookId":"hm",
"legend":{
"type":"tag",
"tagName":"span",
"attrs":{
},
"children":[
"Microcytosis, hypochromia, and target cells consistent with thalassemia."
]
},
"title":{
"type":"tag",
"tagName":"span",
"attrs":{
},
"children":[
"Thalassemia:"
]
},
"footnotes":[

],
"imageInfo":{
