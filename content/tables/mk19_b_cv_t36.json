"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"4cc38d",
"class":"cell txt l",
"children":[
"Visualization of entire aorta and side branches"
]
},
" ",
{
"type":"p",
"hlId":"321644",
"class":"cell txt l",
"children":[
"Sensitivity and specificity >98%"
]
},
" ",
{
"type":"p",
"hlId":"bd4dd2",
"class":"cell txt l",
"children":[
"No exposure to radiation or iodinated contrast dye"
]
},
" "
]
},
{
"type":"tag",
