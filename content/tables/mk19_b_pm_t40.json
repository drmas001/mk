"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"1ca6ee",
"class":"cell txt l",
"children":[
"High-altitude cerebral edema"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"82a633",
"class":"cell txt l",
"children":[
"3000-4000 m"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"61322d",
"class":"cell txt l",
"children":[
"Confusion, irritability, ataxic gait, coma, death"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"264a74",
"class":"cell txt l",
"children":[
"Descent from altitude, supplemental oxygen, hyperbaric therapy"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"64a8ec",
"class":"cell txt l",
"children":[
"Gradual ascent; consider acetazolamide or dexamethasone in patients at high risk"
]
},
" "
]
}
]
},
{
"type":"tag",
"tagName":"tr",
"attrs":{
"className":"group_1-3 row-odd"
},
"children":[
{
"type":"tag",
