"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"bad2e0",
"class":"cell txt l",
"children":[
"Kidney disease"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"d3bad2",
"class":"cell txt l",
"children":[
"Symmetric pitting edema"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"a05d4d",
"class":"cell txt l",
"children":[
"Urinalysis, random urine albumin-creatinine ratio, serum creatinine level"
]
},
" "
]
}
]
},
{
"type":"tag",
"tagName":"tr",
"attrs":{
"className":"group_1-4 row-even"
},
"children":[
{
"type":"tag",
"tagName":"td",
"attrs":{
