"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"21be40",
"class":"cell txt l",
"children":[
"Liver disease"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"d27820",
"class":"cell txt l",
"children":[
"Symmetric pitting edema, ascites, spider angiomas, palmar erythema, jaundice/icterus"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"3391c8",
"class":"cell txt l",
"children":[
"Liver chemistry tests, albumin level, INR"
]
},
" "
]
}
]
},
{
"type":"tag",
"tagName":"tr",
"attrs":{
