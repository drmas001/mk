"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"56ec19",
"class":"cell txt l",
"children":[
"Colchicine"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"f68502",
"class":"cell txt l",
"children":[
"Should be used only if potential benefit justifies potential risk to fetus."
]
},
" "
]
}
]
},
