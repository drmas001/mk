"children":[
" ",
{
"type":"tag",
"tagName":"i",
"attrs":{
},
"children":[
"Cryptosporidium"
]
},
" "
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"0057c1",
"class":"cell txt l",
"children":[
"Watery diarrhea; abdominal cramping; malaise; weight loss"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"aa325a",
"class":"cell txt l",
"children":[
"Modified acid-fast stain; direct fluorescent antibody immunoassay; EIA; NAAT"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"0622b9",
"class":"cell txt l",
"children":[
"Nitazoxanide"
]
},
