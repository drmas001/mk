"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"7319b8",
"class":"cell txt l",
"children":[
"Vasomotor symptoms, cataracts, vascular events (25% lower risk of vascular events than with tamoxifen)"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"38a1d3",
"class":"cell txt l",
"children":[
"Vasomotor symptoms, vaginal dryness, sexual dysfunction, arthralgia, headaches, and insomnia"
]
},
" "
]
},
{
"type":"tag",
"tagName":"td",
"attrs":{
"rowSpan":"1",
"colSpan":"1"
},
"children":[
" ",
{
"type":"p",
"hlId":"5fce2d",
"class":"cell txt l",
"children":[
"Vasomotor symptoms, vaginal dryness, sexual dysfunction, arthralgia, carpal tunnel syndrome, dry eyes, and hypertension"
]
},
" "
]
}
]
